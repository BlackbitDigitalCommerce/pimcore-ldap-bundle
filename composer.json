{
    "name": "blackbit_digital_commerce/pimcore-ldap",
    "type": "pimcore-bundle",
    "description": "Ldap bundle for Pimcore",
    "keywords": [
      "alep",
      "alexpozzi",
      "pimcore",
      "ldap"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
      {
        "name": "Alessandro Pozzi",
        "email": "alexpozzi87@gmail.com",
        "role": "Developer"
      },
      {
        "name": "Blackbit",
        "email": "info@blackbit.de",
        "role": "Developer"
      }
    ],
    "require": {
      "php": ">=7.0",
      "ext-ldap": "*",
      "symfony/symfony": ">=3.4.3",
      "symfony/ldap": ">=3.4.3",
      "pimcore/pimcore": ">=5.1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.5",
      "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4" : {
          "Alep\\LdapBundle\\" : "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Alep\\LdapBundle\\tests\\": "tests"
      }
    }
}
